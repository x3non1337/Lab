#include<stdio.h>
#include<math.h>
double get_power(double x, long y)
{
if (y == 0) 
{
  return 1;
}
if (y < 0) 
{
  return get_power(1.0 / x, -y);
}
return x * get_power(x, y - 1);

}

int main()
{
double x;

long y;

while (scanf("%lf %ld", &x, &y) == 2)

{

printf("%lf\n", get_power(x, y));
}

}
